<div class="container mt-50">
  <div class="container mt-5">
    <div class="container">
    追跡番号を登録
    </div>
  </div>
  <div class="container mt-5">
    <%= form_with model: @kaigai_tracking_number, url: m_new_order_tracking_create_manage_path, local: true do |f| %>
    <%= render 'shared/error_messages', model: f.object %> 
    <div class="weight-bold-text">
        追跡番号
        <span class="indispensable">必須</span>
      </div>
      <%= f.text_field :number, class:"items-text", id:"info-order-tracking-number", placeholder:"追跡番号を入力" %>
    <%= f.submit "登録する", class: "btn btn-warning mt-2"%>
    <% end %>
  </div>
</div>